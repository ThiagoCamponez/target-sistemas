Algoritmo "Exercício 1"
var
   indice, soma, k: inteiro
Inicio
   indice <- 13
   soma <- 0
   k <- 0
   enquanto (k < indice) faca
            k <- k + 1
            soma <- soma + k
   fimenquanto
   Escreva ("No final do processamento, o valor da variável SOMA é: ", soma, ".")
Fimalgoritmo